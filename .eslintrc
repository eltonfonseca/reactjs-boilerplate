{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "airbnb"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 11,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "react/jsx-filename-extension": "off",
      "linebreak-style": 0,
      "import/no-unresolved": "off",
      "import/prefer-default-export": "off"
  }
}